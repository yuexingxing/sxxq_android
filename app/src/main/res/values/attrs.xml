<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="LineControllerView">
        <!-- 名称 -->
        <attr name="name" format="string" />
        <!-- 内容或当前状态 -->
        <attr name="subject" format="string" />
        <!-- 是否是列表中最后一个 -->
        <attr name="showTopLine" format="boolean" />
        <attr name="showBottomLine" format="boolean" />
        <!-- 是否可以跳转 -->
        <attr name="canNav" format="boolean" />
    </declare-styleable>
    <declare-styleable name="OrderTabMenuView">
        <attr name="title" format="string" />
        <attr name="isShowLine" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PersonalAccountView">
        <attr name="icon_pav" format="reference" />
        <attr name="title_pav" format="string" />
        <attr name="content_pav" format="string" />
        <attr name="isShowLine_pav" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <!--未选中-->
        <attr name="starEmptyRes" format="reference" />
        <!--选中半个-->
        <attr name="starHalfRes" format="reference" />
        <!--选中全部-->
        <attr name="starSelectedRes" format="reference" />
        <!--星星的总数-->
        <attr name="startTotalNumber" format="integer" />
        <!--默认选中的数量 -->
        <attr name="selectedNumber" format="float" />
        <!--星星的间距-->
        <attr name="starDistance" format="dimension" />
        <!--星星的宽度-->
        <attr name="starWidth" format="dimension" />
        <!--星星的高度-->
        <attr name="starHeight" format="dimension" />
        <!--是不是只画整个星星，不画半个-->
        <attr name="starIsFull" format="boolean" />
        <!--是否可选择-->
        <attr name="select_enable" format="boolean" />
    </declare-styleable>
    <declare-styleable name="OvalImageView">
        <attr name="OvalImageView_top_left" format="integer" />
        <attr name="OvalImageView_top_right" format="integer" />
        <attr name="OvalImageView_bottom_left" format="integer" />
        <attr name="OvalImageView_bottom_right" format="integer" />
    </declare-styleable>
    <declare-styleable name="PersonalOrderMenuView">
        <attr name="PersonalOrderMenuView_name" format="string" />
        <attr name="PersonalOrderMenuView_icon" format="reference" />
        <attr name="PersonalOrderMenuView_num" format="integer" />
    </declare-styleable>
    <declare-styleable name="EmptyLayout">
        <attr name="elEmptyLayout"  format="reference"/>
        <attr name="elErrorLayout"  format="reference"/>
        <attr name="elLoadingLayout"  format="reference"/>
    </declare-styleable>
</resources>